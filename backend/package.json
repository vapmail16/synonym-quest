{
  "name": "synonym-quest-backend",
  "version": "1.0.0",
  "description": "Backend API for Synonym Trainer",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:badges": "jest tests/unit/models/Badge.test.ts",
    "seed:badges": "ts-node src/scripts/seed-badges.ts",
    "test:legacy": "node run-tests.js",
    "test:auth": "node run-tests.js auth-test-suite.js",
    "test:frontend": "node run-tests.js frontend-crash-test.js",
    "test:speed": "node run-tests.js speed-round-test.js",
    "test:suite": "node run-tests.js test-suite.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "nock": "^14.0.10",
    "nodemon": "^3.0.2",
    "pg-mem": "^3.0.5",
    "prettier": "^3.1.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "synonym",
    "vocabulary",
    "education",
    "api"
  ],
  "author": "Vikkas Arun Pareek",
  "license": "MIT"
}
